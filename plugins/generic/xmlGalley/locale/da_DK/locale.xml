<locale name="da_DK" full_name="Danish">
	<message key="plugins.generic.xmlGalley.displayName">XML korrektur plugin</message>
	<message key="plugins.generic.xmlGalley.description">Dette plugin genererer XHTML korrekturer fra en XML artikel ved hjælp af XSLT.</message>

	<message key="plugins.generic.xmlGalley.manager.settings">Indstillinger</message>

	<!-- XML Article Galleys Settings Management -->
	<message key="plugins.generic.xmlGalley.settings.description"><![CDATA[<p>Med dette plugin bliver korrekturversioner af artikler automatisk genereret fra en uploaded XML kilde ved hjælp af em særlig XSL stylesheet transformation.</p>]]></message>
	<message key="plugins.generic.xmlGalley.settings.renderer">XSLT Rendering Method</message>
	<message key="plugins.generic.xmlGalley.settings.PHP5">PHP 5.0.0+ med XSL funktioner (libxslt)</message>
	<message key="plugins.generic.xmlGalley.settings.PHP4">PHP 4.0.3+ med XSLT funktioner (Sablotron)</message>
	<message key="plugins.generic.xmlGalley.settings.notAvailable">Ikke tilgængelig</message>

	<message key="plugins.generic.xmlGalley.settings.externalXSLT">Ekstern kommando-linie XSLT gengivelse (f.eks. Xalan)</message>
	<message key="plugins.generic.xmlGalley.settings.externalXSLTDescription"><![CDATA[Indtast den fulde sti til XSLT genereringsværktøjet med alle krævede parametre.  Brug %xsl til at erstatte placeringen af XSL stylesheet filen, og %xml til at erstatte placeringen af XML kildefilen; f.eks.: <br/><pre>/usr/bin/java -jar ~/java/xalan.jar -HTML -IN %xml -XSL %xsl</pre>]]></message>
	<message key="plugins.generic.xmlGalley.settings.externalXSLTTest">Test XSLT</message>
	<message key="plugins.generic.xmlGalley.settings.externalXSLTSuccess">Ekstern XSLT test lykkedes.</message>
	<message key="plugins.generic.xmlGalley.settings.externalXSLTFailure">Ekstern XSLT test mislykkedes.</message>
	<message key="plugins.generic.xmlGalley.settings.externalXSLTRequired">Der skal angives en ekstern kommando-linie XSLT processor.</message>

	<message key="plugins.generic.xmlGalley.settings.stylesheet">XSL Stylesheet</message>
	<message key="plugins.generic.xmlGalley.settings.xslNLM"><![CDATA[<a href="http://dtd.nlm.nih.gov/publishing/">NLM Journal Publishing DTD</a> → XHTML]]></message>

	<message key="plugins.generic.xmlGalley.settings.xslFOP">Aktiver generering af PDF korrekturer ved hjælp af XSL-FO (f.eks. FOP)</message>
	<message key="plugins.generic.xmlGalley.settings.xslFOPDescription"><![CDATA[Indtast den fulde sti til FO processor med alle krævede paramre. Brug %fo til at erstatte placeringen af XSL-FO filen, og %pdf til at erstatte placeringen af PDF filen; f.eks.: <br/><pre>/usr/sbin/fop -fo %fo -pdf %pdf</pre>]]></message>
	<message key="plugins.generic.xmlGalley.settings.xslFOPRequired">Der skal angives en ekstern kommando-linie FO processor.</message>

	<message key="plugins.generic.xmlGalley.settings.customXSL">Standard XSL stylesheet</message>
	<message key="plugins.generic.xmlGalley.settings.customXSLRequired">En standard XSL stylesheet fil skal angives.</message>
	<message key="plugins.generic.xmlGalley.settings.customXSLInvalid">Den uploadede XSL fil var ugyldig. Check filen og prøv igen.</message>

</locale>

