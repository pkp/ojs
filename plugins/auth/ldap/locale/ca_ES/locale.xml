	<message key="plugins.auth.ldap.settings.saslopt">Configuració de SASL (opcional)</message>
	<message key="plugins.auth.ldap.settings.saslmech">Mecanisme SASL</message>
	<message key="plugins.auth.ldap.settings.saslmech.description">P. ex., "DIGEST-MD5"</message>
	<message key="plugins.auth.ldap.settings.saslrealm">Domini</message>
	<message key="plugins.auth.ldap.settings.saslauthzid">Id. de l'autorització sol·licitada</message>
