msgid "plugins.auth.ldap.displayName"
msgstr "LDAP"

msgid "plugins.auth.ldap.description"
msgstr " Plugin ini mengizinkan pengesahan dan sinkronisasi akun pengguna terhadap sumber data LDAP."

msgid "plugins.auth.ldap.settings"
msgstr "Pengaturan LDAP"

msgid "plugins.auth.ldap.settings.hostname"
msgstr " Hostname Server"

msgid "plugins.auth.ldap.settings.hostname.description"
msgstr " Contoh: \"ldap.example.com\", atau \"ldaps://ldap.example.com\" (untuk menggunakan SSL)"

msgid "plugins.auth.ldap.settings.port"
msgstr "Server Port"

msgid "plugins.auth.ldap.settings.port.description"
msgstr " Opsional. Default untuk 389 (LDAP) atau 636 (LDAP over SSL)"

msgid "plugins.auth.ldap.settings.basedn"
msgstr "Base DN"

msgid "plugins.auth.ldap.settings.basedn.description"
msgstr " Contoh:\"ou=people,dc=example,dc=com\""

msgid "plugins.auth.ldap.settings.managerdn"
msgstr " DN Manajer"

msgid "plugins.auth.ldap.settings.managerdn.description"
msgstr " Contoh: \"cn=Manager,dc=example,dc=com\""

msgid "plugins.auth.ldap.settings.managerpwd"
msgstr " Kata Sandi Manajer"

msgid "plugins.auth.ldap.settings.managerpwd.description"
msgstr " DN Manajer dan kata sandi hanya diperlukan jika sinkronisasi profil/kata sandi pengguna atau pilihan kreasi pengguna diaktifkan. Jika LDAP akan digunakan hanya untuk pengesahan, maka opsi ini dapat dihapus."

msgid "plugins.auth.ldap.settings.uid"
msgstr " Atribut Nama Akun"

msgid "plugins.auth.ldap.settings.uid.description"
msgstr " Atribut yang nilanya secara unik mengidentifikasi obyek pengguna, seperti uid atau cn atau sAMAccountName."

msgid "plugins.auth.ldap.settings.pwhash"
msgstr " Enkripsi Kata Sandi "

msgid "plugins.auth.ldap.settings.pwhash.description"
msgstr " Format hash untuk kata sandi disimpan di server. SSHA direkomendasikan (memerlukan PHP>=4.3.0.)"

msgid "plugins.auth.ldap.settings.saslopt"
msgstr " Pengaturan SASL (opsional)"

msgid "plugins.auth.ldap.settings.sasl"
msgstr " Gunakan SASL sebagai pengganti pengesahan yang sederhana (memerlukan PHP>=5)"

msgid "plugins.auth.ldap.settings.saslmech"
msgstr " Mekanisme SASL"

msgid "plugins.auth.ldap.settings.saslmech.description"
msgstr " Contoh: \"DIGEST-MD5\""

msgid "plugins.auth.ldap.settings.saslrealm"
msgstr " Jangkauan "

msgid "plugins.auth.ldap.settings.saslauthzid"
msgstr " Memerlukan ID Pengesahan "

msgid "plugins.auth.ldap.settings.saslprop"
msgstr " SASL Properti Keamanan "
