<locale name="id_ID" full_name="Bahasa Indonesia">
	<message key="plugins.pubIds.urn.displayName">URN</message>

	<!-- settings form -->
	<message key="plugins.pubIds.urn.manager.settings.urnObjects">Konten Jurnal</message>
	<message key="plugins.pubIds.urn.manager.settings.urnObjectsRequired">Pilih objek URN yang harus ditugaskan.</message>
	<message key="plugins.pubIds.urn.manager.settings.enableIssueURN">URN akan ditugaskan untuk terbitan.</message>
	<message key="plugins.pubIds.urn.manager.settings.enableSubmissionURN">URN akan ditugaskan untuk artikel.</message>
	<message key="plugins.pubIds.urn.manager.settings.enableRepresentationURN">URN akan ditugaskan untuk galley (contoh: HTML atau file PDF).</message>

	<message key="plugins.pubIds.urn.manager.settings.urnPrefix">Awalan URN</message>
	<message key="plugins.pubIds.urn.manager.settings.urnPrefix.description"><![CDATA[Awalan URN sudah baku, jangan pernah mengubah bagian URN (contoh "urn:nbn:de:0000-").]]></message>
	<message key="plugins.pubIds.urn.manager.settings.form.urnPrefixPattern"><![CDATA[Pola akhiran URN harus dalam format "urn:"&lt;NID&gt;":"&lt;NSS&gt;.]]></message>

	<message key="plugins.pubIds.urn.manager.settings.urnSuffix">Akhiran URN</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSuffixPattern.example">Sebagai contoh, vol%viss%ipp%p dapat menciptakan akhiran URN "vol3iss2pp230".</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSuffixPattern.issues">untuk terbitan</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSuffixPattern.submissions">untuk artikel</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSuffixPattern.representations">untuk galley</message>
	<message key="plugins.pubIds.urn.manager.settings.urnIssueSuffixPatternRequired">Silahkan masukkan pola akhiran URN untuk terbitan.</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSubmissionSuffixPatternRequired">>Silahkan masukkan pola akhiran URN untuk artikel.</message>
	<message key="plugins.pubIds.urn.manager.settings.urnRepresentationSuffixPatternRequired">>Silahkan masukkan pola akhiran URN untuk galley.</message>
	<message key="plugins.pubIds.urn.manager.settings.urnSuffixDefault">Gunakan pola default.</message>
	<message key="plugins.pubIds.urn.manager.settings.checkNo">Cek Nomor</message>
	<message key="plugins.pubIds.urn.manager.settings.checkNo.label">Cek nomor akan secara otomatis terhitung dan ditambahkan di akhir, sebagai digit terakhir URN.</message>
	<message key="plugins.pubIds.urn.manager.settings.namespace">Namespace</message>
	<message key="plugins.pubIds.urn.manager.settings.namespace.choose">Pilih</message>
	<message key="plugins.pubIds.urn.manager.settings.namespace.description">Biasanya Identifier namespace akan terus menerus diperlukan untuk pendaftaran (contoh: di the Deutsche Nationalbibliothek).</message>

	<message key="plugins.pubIds.urn.manager.settings.urnReassign">Hapus URN</message>
	<message key="plugins.pubIds.urn.manager.settings.clearURNs.description">Jika anda mengganti konfigurasi URN, tidak akan berpengaruh terhadap URN yang telah ditetapkan sebelumnya. Sekali konfigurasi URN telah disimpan, gunakan tombol ini untuk membersihkan semua URN yang ada sehingga pengaturan baru akan berlaku pada artikel yang ada.</message>
	<message key="plugins.pubIds.urn.manager.settings.urnReassign.confirm">Anda yakin untuk menghapus semua URN yang ada?</message>

	<!-- metadata form -->
	<message key="plugins.pubIds.urn.editor.urn">URN</message>
	<message key="plugins.pubIds.urn.editor.urnSuffixCustomIdentifierNotUnique">Akhiran URN yang dimasukkan telah digunakan oleh item penerbitan lain. Silahkan masukkan akhiran URN unik untuk masing-masing item.</message>
	<!-- errors -->
	<message key="plugins.pubIds.urn.form.checkNoRequired">Cek nomor hilang.</message>

</locale>
