msgstr "Idatzi fitxategi osagarri honen izenburua."

msgid "author.submit.suppFile.form.subjectRequired"
msgstr "Sartu gaia."

msgid "author.submit.suppFile.form.descriptionRequired"
msgstr "Sartu azalpena."

