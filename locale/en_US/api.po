msgid "api.submissions.403.cantChangeContext"
msgstr "You can not change the server of a submission."

msgid "api.submissions.403.contextRequired"
msgstr "To make or edit a submission you must make a request to the server's API endpoint."

msgid "api.contexts.403.requestedDisabledContexts"
msgstr "You can only view servers that have been enabled."

msgid "api.contexts.403.notAllowed"
msgstr "You do not have permission to view this server."

msgid "api.contexts.403.contextsDidNotMatch"
msgstr "You can not view or edit this server unless you make a request to the server's API or the site-wide API."

msgid "api.contexts.403.requiresContext"
msgstr "You can not edit this server from the site-wide API."

msgid "api.contexts.403.notAllowedEdit"
msgstr "You do not have permission to edit this server."

msgid "api.contexts.403.notAllowedDelete"
msgstr "You do not have permission to delete this server."

msgid "api.contexts.404.contextNotFound"
msgstr "The server you requested was not found."

msgid "api.emailTemplates.403.notAllowedChangeContext"
msgstr "You do not have permission to move this email template to another server."

msgid "api._payments.400.invalidPaymentMethod"
msgstr "The payment method you selected is not supported."

msgid "api.publications.403.contextsDidNotMatch"
msgstr "The publication that you requested is not part of this server."

msgid "api.publications.403.submissionsDidNotMatch"
msgstr "The publication that you requested is not part of this submission."
